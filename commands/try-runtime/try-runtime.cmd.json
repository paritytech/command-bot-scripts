{
  "$schema": "../../schema.cmd.json",
  "command": {
    "description": "Runs `cargo run --release --quiet --features=try-runtime try-runtime --chain=<CHAIN> --execution=Wasm --no-spec-check-panic on-runtime-upgrade live --uri=<URI>`",
    "configuration": { "gitlab": { "job": { "tags": ["linux-docker"] }, "variables": {} } },
    "presets": {
      "default": {
        "args": {
          "chain":  { "label": "Chain", "rule": "/^[a-z_-]+$/" },
          "uri":    { "label": "URI", "rule": [".*"] }
        }
      }
    }
  }
}
